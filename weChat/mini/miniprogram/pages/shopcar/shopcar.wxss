.none{
    text-align:center;
    padding:40rpx 0;
}
.none image{
    width:200rpx;
    height:200rpx;
    margin-bottom:60rpx;
}
.none view{
    padding-bottom: 20rpx;
    color:#888;
}
.none text{
    color:#ccc;
    font-size:14px;
}
.cars{
    background-color:#eee;
    min-height: 100vh;
    padding-bottom:80rpx;
}
.car{
    margin:10rpx 0;
    padding:10rpx;
    background-color:#fff;
}
.car .check{
    line-height:160rpx;
}
.img image{
    width:160rpx;
    height:160rpx;
    border-radius: 20rpx;
}
.mes{
    position:relative;
    flex:1;
    height:160rpx;
    padding:0 10rpx;
}
.mes .top{
    color:#333;
    word-wrap: break-word;
    font-size:12px;
}
.mes .bottom{
    width:100%;
    position:absolute;
    bottom:0;
    text-align:right;
    padding:0 20rpx;
}
.mes .bottom text{
    float:left;
    line-height:68rpx;
    color:#f44;
}
.mes .bottom button{
    display:inline-block;
    padding:10rpx;
    color:#333;
    line-height:40rpx;
    width:60rpx;
    height:60rpx;
    min-height: 60rpx;
    border-radius: 10rpx;
}
.mes .bottom input{
    display:inline-block;
    width:100rpx;
    text-align:center;
    border: 1px solid #f8f8f8;
    background-color: #f8f8f8;
    height: 60rpx;
    margin: 0 10rpx;
}
.shop{
    width:100%;
    position:fixed;
    bottom:0;
    background-color:#fff;
}
.shop .checkall{
    padding:10rpx;
    font-size:14px;
    flex:1;
}
.shop .total{
    font-size:12px;
    color:#333;
    line-height: 68rpx;
}
.shop .total text{
    color:#f44;
}
.shop .todo{
    line-height: 68rpx;
}
.shop .todo text{
    padding:12rpx 20rpx;
    background-color: #ddd;
    border-radius:40rpx;
    margin:0 10rpx;
}
.shop .todo.success text{
    background-color:#f44;
}